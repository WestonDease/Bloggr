  "devDependencies": {
    "concurrently": "^3.5.0",
    "nodemon": "^1.11.0"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "babel-polyfill": "^6.26.0",
    "draft-js": "^0.10.5",
    "electron": "^4.0.3",
    "express": "^4.16.3",
    "fs": "0.0.1-security",
    "if-env": "^1.0.4",
    "js-crawler": "^0.3.21",
    "mongoose": "^5.0.17",
    "nightmare": "^3.0.1",
    "path": "^0.12.7",
    "react": "^16.7.0",
    "react-dom": "^16.7.0"
  }
